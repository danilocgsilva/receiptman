#!/usr/bin/env php
<?php

use Symfony\Component\Console\Application;
use App\Command\ReceiptDebianCommand;
use App\Command\PhpFullDev;
use App\Command\Python;
use App\Command\Node;

require_once './vendor/autoload.php';

$application = new Application();
$application->add(new PhpFullDev());
$application->add(new ReceiptDebianCommand());
$application->add(new Python());
$application->add(new Node());

$application->run();
